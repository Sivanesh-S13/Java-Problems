import java.util.*;
import java.io.*;
class thief
{
    public static void main(String args[])
    {
        Scanner s=new Scanner(System.in);
        int size,sum=0;
        int arr[]=new int[size];
        for(int k:arr)
        arr[k]=s.nextInt();
        for(int i=0;i<length.arr;i+2)
        sum+=arr[i];
        System.out.println(sum);


    }
}